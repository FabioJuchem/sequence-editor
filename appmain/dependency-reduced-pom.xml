  <modelVersion>4.0.0</modelVersion>
  <artifactId>appmain</artifactId>
  <name>appmain</name>
  <version>0.0.0.1</version>
  <description>Demo project for Spring Boot</description>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <executions>
          <execution>
            <id>create-fat-jar</id>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <transformers>
                <transformer>
                  <mainClass>com.fabiojuchem.appmain.AppmainApplicationKt</mainClass>
                </transformer>
              </transformers>
              <finalName>A</finalName>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
